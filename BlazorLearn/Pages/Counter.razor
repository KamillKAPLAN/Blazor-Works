@page "/counter"

<h1>Counter</h1>

<p>Current count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">one</button>
<button class="btn btn-primary" @onclick="IncrementCount2">two</button>

@code {
    private int currentCount = 0;

    void IncrementCount()
    {
        currentCount++;
    }

    void IncrementCount2()
    {
        currentCount += 2;
    }
}
